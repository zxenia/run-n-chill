   "source": [
    "#merge pdf files in one pdf\n",
    "from PyPDF2 import PdfFileMerger\n",
    "\n",
    "pdfs = ['01.pdf', '02.pdf', '03.pdf', '04.pdf', '05.pdf']\n",
    "\n",
    "merger = PdfFileMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(open(pdf, 'rb'))\n",
    "\n",
    "with open('merged.pdf', 'wb') as fout:\n",
    "    merger.write(fout)"
   ]
  }
